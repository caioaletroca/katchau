{
	"name": "katchau",
	"version": "0.1.0",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"start:cloud": "supabase start",
		"stop:cloud": "supabase stop",
		"analyze": "cross-env ANALYZE=true next build",
		"lint": "next lint --fix",
		"prettier": "prettier --write ./src",
		"test:watch": "jest --watch",
		"test": "jest",
		"cypress:open": "cypress open",
		"extract:i18n": "formatjs extract \"./src/**/*.{js,ts,tsx}\" --format simple --out-file \"./locales/extract/en.json\" --throws",
		"compile:i18n": "formatjs compile-folder \"./locales/extract\" --ast --format simple \"./locales/compiled\"",
		"i18n": "npm run extract:i18n & npm run compile:i18n",
		"seed": "prisma db seed",
		"migrate:dev": "prisma migrate dev",
		"migrate:reset": "prisma migrate reset",
		"prepare": "husky install",
		"postinstall": "prisma generate --quiet"
	},
	"prisma": {
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seeds/index.ts"
	},
	"dependencies": {
		"@auth/firebase-adapter": "^1.0.0",
		"@auth/prisma-adapter": "^1.0.0",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@formatjs/intl-localematcher": "^0.4.0",
		"@mui/lab": "^5.0.0-alpha.138",
		"@mui/material": "^5.13.6",
		"@mui/x-date-pickers": "^6.9.1",
		"@prisma/client": "^4.16.1",
		"@supabase/supabase-js": "^2.26.0",
		"@types/lodash": "^4.14.195",
		"@types/node": "20.3.1",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"autoprefixer": "10.4.14",
		"axios": "^1.4.0",
		"bcrypt": "^5.1.0",
		"dayjs": "^1.11.9",
		"dotenv": "^16.3.1",
		"encoding": "^0.1.13",
		"eslint": "8.43.0",
		"eslint-config-next": "13.4.7",
		"firebase": "^9.23.0",
		"firebase-admin": "^11.9.0",
		"formik": "^2.4.2",
		"formik-validator-zod": "^1.0.2",
		"lodash": "^4.17.21",
		"material-symbols": "^0.8.1",
		"negotiator": "^0.6.3",
		"next": "13.4.7",
		"next-auth": "^4.22.1",
		"path-to-regexp": "^6.2.1",
		"postcss": "8.4.24",
		"react": "18.2.0",
		"react-device-detect": "^2.2.3",
		"react-dom": "18.2.0",
		"react-dropzone": "^14.2.3",
		"react-easy-crop": "^5.0.0",
		"react-intl": "^6.4.4",
		"swr": "^2.2.0",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.3",
		"uuid": "^9.0.0",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.0.2",
		"@formatjs/cli": "^6.1.3",
		"@next/bundle-analyzer": "^13.4.7",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/negotiator": "^0.6.1",
		"@types/uuid": "^9.0.2",
		"babel-plugin-formatjs": "^10.5.3",
		"cross-env": "^7.0.3",
		"cypress": "^12.17.3",
