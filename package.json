		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"lint-staged": "^13.2.2",
		"next-compose-plugins": "^2.2.1",
		"prettier": "^2.8.8",
		"prettier-plugin-organize-imports": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"pretty-quick": "^3.1.3",
		"prisma": "^4.16.1",
		"supabase": "^1.75.2",
		"ts-node": "^10.9.1"
	}
}
