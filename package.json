    "@mui/material": "^5.13.6",
    "@prisma/client": "^4.16.1",
    "@supabase/supabase-js": "^2.26.0",
    "@types/lodash": "^4.14.195",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "encoding": "^0.1.13",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "firebase": "^9.23.0",
