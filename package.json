{
  "name": "katchau",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix",
    "prettier": "prettier --write ./src",
		"extract:i18n": "formatjs extract \"./src/app/**/*.{js,ts,tsx}\" --format simple --out-file \"./locales/extract/en.json\" --throws",
		"compile:i18n": "formatjs compile-folder \"./locales/extract\" --ast --format simple \"./locales/compiled\"",
		"i18n": "npm run extract:i18n & npm run compile:i18n",
    "migrate:dev": "prisma migrate dev",
    "prepare": "husky install"
  },
  "dependencies": {
