    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-easy-crop": "^5.0.0",
    "react-intl": "^6.4.4",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3",
    "uuid": "^9.0.0",
